sudo apt update -y && sudo apt upgrade -y
sudo apt install -y geany efibootmgr vlc deluge partitionmanager git flatpak neofetch libgdk-pixbuf-xlib-2.0-0 libgdk-pixbuf2.0-0 libopenal-data libopenal1 gnome-tweaks
flatpak remote-add --if-not-exists flathub https://dl.flathub.org/repo/flathub.flatpakrepo
flatpak install flathub io.github.peazip.PeaZip
wget https://www.xnview.com/download.php?file=XnViewMP-linux-x64.deb
sudo dpkg -i 'download.php?file=XnViewMP-linux-x64.deb'
sudo rm 'download.php?file=XnViewMP-linux-x64.deb'
sudo apt purge -y gnome-text-editor evince gnome-font-viewer eog gnome-characters seahorse gnome-font-viewer gedit papers gnome-disk-utility gnome-calculator imagemagick
sudo apt autoremove -y
